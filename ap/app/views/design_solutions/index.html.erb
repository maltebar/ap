<p id="notice"><%= notice %></p>

<h1>Listing Design Solutions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @design_solutions.each do |design_solution| %>
      <tr>
        <td><%= link_to 'Show', design_solution %></td>
        <td><%= link_to 'Edit', edit_design_solution_path(design_solution) %></td>
        <td><%= link_to 'Destroy', design_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Design solution', new_design_solution_path %>
