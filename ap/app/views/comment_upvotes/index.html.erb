<p id="notice"><%= notice %></p>

<h1>Listing Comment Upvotes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_upvotes.each do |comment_upvote| %>
      <tr>
        <td><%= link_to 'Show', comment_upvote %></td>
        <td><%= link_to 'Edit', edit_comment_upvote_path(comment_upvote) %></td>
        <td><%= link_to 'Destroy', comment_upvote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment upvote', new_comment_upvote_path %>
