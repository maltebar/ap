<p id="notice"><%= notice %></p>

<h1>Listing Code Case Attachments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_case_attachments.each do |code_case_attachment| %>
      <tr>
        <td><%= link_to 'Show', code_case_attachment %></td>
        <td><%= link_to 'Edit', edit_code_case_attachment_path(code_case_attachment) %></td>
        <td><%= link_to 'Destroy', code_case_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

