<p id="notice"><%= notice %></p>

<h1>Listing Code Solutions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_solutions.each do |code_solution| %>
      <tr>
        <td><%= link_to 'Show', code_solution %></td>
        <td><%= link_to 'Edit', edit_code_solution_path(code_solution) %></td>
        <td><%= link_to 'Destroy', code_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code solution', new_code_solution_path %>
