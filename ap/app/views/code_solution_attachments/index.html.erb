<p id="notice"><%= notice %></p>

<h1>Listing Code Solution Attachments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_solution_attachments.each do |code_solution_attachment| %>
      <tr>
        <td><%= link_to 'Show', code_solution_attachment %></td>
        <td><%= link_to 'Edit', edit_code_solution_attachment_path(code_solution_attachment) %></td>
        <td><%= link_to 'Destroy', code_solution_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

