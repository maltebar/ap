<p id="notice"><%= notice %></p>

<h1>Listing Design Cases</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @design_cases.each do |design_case| %>
      <tr>
        <td><%= link_to 'Show', design_case %></td>
        <td><%= link_to 'Edit', edit_design_case_path(design_case) %></td>
        <td><%= link_to 'Destroy', design_case, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Design case', new_design_case_path %>
